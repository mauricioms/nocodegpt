{
  "name": "front-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:e2e": "start-server-and-test preview https://manager.mauricioresiduos.app.br 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore -f ./node_modules/eslint-junit/index.js -o eslint-junit.xml",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/chart.js": "^2.9.37",
    "chart.js": "^4.3.3",
    "maska": "^2.1.10",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "split-grid": "^1.0.11",
    "split.js": "^1.6.5",
    "underscore": "^1.13.6",
    "vue": "^3.4.21",
    "vue-draggable-next": "^2.2.1",
    "vue-router": "^4.3.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/chartjs": "^0.0.31",
    "@types/google.maps": "^3.54.6",
    "@types/jsdom": "^21.1.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^18.17.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^13.6.6",
    "eslint": "^8.45.0",
    "eslint-html-reporter": "^0.7.4",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.15.1",
    "i": "^0.3.7",
    "jsdom": "^22.1.0",
    "npm": "^10.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "latest",
    "typescript-formatter": "^7.2.2",
    "vite": "^4.4.6",
    "vite-plugin-vue-devtools": "^7.0.20",
    "vitest": "^0.33.0",
    "vue-tsc": "^2.1.6"
  }
}
